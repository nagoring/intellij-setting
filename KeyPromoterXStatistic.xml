<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F3'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Ctrl+Alt+L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>13</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>